{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "gui:pretest": "npm run gui:build",
    "gui:test": "xo && ava",
    "gui:build": "tsc && chmod +x dist/gui/cli.js",
    "gui:start": "npm run build && npm run gui:build && dist/gui/cli.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "worldeater",
  "author": "LordSequoia",
  "module": "dist/worldeater.esm.js",
  "size-limit": [
    {
      "path": "dist/worldeater.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/worldeater.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@sindresorhus/tsconfig": "^3.0.1",
    "@size-limit/preset-small-lib": "^8.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/react": "^18.0.17",
    "@types/tail": "^2.2.1",
    "ava": "^4.3.1",
    "chalk": "^5.0.1",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "ink-testing-library": "^2.1.0",
    "size-limit": "^8.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "xo": "^0.51.0"
  },
  "dependencies": {
    "effector": "^22.3.0",
    "effector-logger": "^0.13.4",
    "fs-extra": "^10.1.0",
    "ink": "^3.2.0",
    "keyv": "^4.3.3",
    "keyv-file": "^0.2.0",
    "meow": "^10.1.3",
    "microdiff": "^1.3.1",
    "micromatch": "^4.0.5",
    "patronum": "^1.10.0",
    "react": "^18.2.0",
    "rxjs": "^7.5.6",
    "tail": "^2.2.4",
    "type-fest": "^2.18.0",
    "winston": "^3.8.1"
  }
}
